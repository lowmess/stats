  },
  "author": "Alec Lomas <alec@lowmess.com>",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.18",
    "@types/node-fetch": "^2.6.1",
    "@types/xml2js": "^0.4.9",
    "abort-controller": "^3.0.0",
    "apollo-server-express": "^3.6.3",
    "apollo-server-plugin-response-cache": "^3.5.1",
    "aws-sdk": "^2.1074.0",
    "date-fns": "^2.28.0",
    "express": "^4.17.2",
    "graphql": "^16.3.0",
    "node-fetch": "^2.6.1",
    "typescript": "^4.5.5",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "dotenv": "^16.0.0",
    "eslint": "^8.9.0",
    "eslint-config-lowmess": "^4.1.4",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "prettier-config-lowmess": "^0.3.0",
    "vercel": "^23.1.2"
